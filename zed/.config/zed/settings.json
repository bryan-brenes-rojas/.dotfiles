{
  "theme": "One Dark",
  "vim_mode": true,
  "relative_line_numbers": true,
  "buffer_line_height": {
    "custom": 1.5
  },
  "cursor_blink": false,
  "languages": {
    "TypeScript": {
      "code_actions_on_format": {
        "source.organizeImports": true
      }
    },
    "TSX": {
      "code_actions_on_format": {
        "source.organizeImports": true
      }
    }
  },
  "terminal": {
    "copy_on_select": true
  }
}
