{
  "coc.preferences.formatOnSaveFiletypes": [
    "lua",
    "css",
    "markdown",
    "typescript",
    "typescriptreact",
    "javascript",
    "html",
    "py",
    "rb",
    "json",
    "yaml"
  ],
  "explorer.file.root.template": "[icon] [title] [git & 1][hidden & 1][root] [fullpath]",
  "explorer.icon.enableNerdfont": true,
  "explorer.git.icon.status.added": "",
  "explorer.git.icon.status.modified": "ﰣ",
  "explorer.git.icon.status.ignored": "",
  "explorer.git.icon.status.untracked": "",
  "explorer.git.icon.status.renamed": "",
  "explorer.git.icon.status.unmerged": "",
  "explorer.file.showHiddenFiles": true,
  "diagnostic.checkCurrentLine": true,
  "diagnostic.messageDelay": 500,
  "diagnostic.errorSign": "",
  "diagnostic.infoSign": "",
  "diagnostic.hintSign": "",
  "diagnostic.warningSign": "",
  "diagnostic.enableMessage": "jump",
  "diagnostic.virtualText": true,
  "diagnostic.virtualTextPrefix": "  ",
  "solargraph.formatting": true,
  "solargraph.diagnostics": true,
  "sumneko-lua.enableNvimLuaDev": true,
  "Lua.telemetry.enable": false,
  "outline.keepWindow": true,
  "outline.splitCommand": "botright 80vs",
  "outline.expandLevel": 1,
  "outline.sortBy": "position",
  "suggest.completionItemKindLabels": {
    "keyword": "\uf1de",
    "variable": "\ue79b",
    "value": "\uf89f",
    "operator": "\u03a8",
    "constructor": "\uf0ad",
    "function": "\u0192",
    "reference": "\ufa46",
    "constant": "\uf8fe",
    "method": "\uf09a",
    "struct": "\ufb44",
    "class": "\uf0e8",
    "interface": "\uf417",
    "text": "\ue612",
    "enum": "\uf435",
    "enumMember": "\uf02b",
    "module": "\uf40d",
    "color": "\ue22b",
    "property": "\ue624",
    "field": "\uf9be",
    "unit": "\uf475",
    "event": "\ufacd",
    "file": "\uf723",
    "folder": "\uf114",
    "snippet": "\ue60b",
    "typeParameter": "\uf728",
    "default": "\uf29c"
  },
  "suggest.virtualText": true,
  "suggest.formatItems": ["kind", "abbr", "menu", "shortcut"],
  "typescript.preferences.importModuleSpecifier": "relative"
}
