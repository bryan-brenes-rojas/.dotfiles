{
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "markdown",
    "typescript",
    "javascript",
    "html",
    "py",
    "rb",
    "json",
    "yaml"
  ],
  "explorer.file.root.template": "[icon] [title] [git & 1][hidden & 1][root] [fullpath]",
  "explorer.icon.enableVimDevicons": true,
  "explorer.icon.enableNerdfont": true
}
